{
    "name": "ay4t/ci4-dashboard",
    "description": "Create a Dashboard Libraries for Codeigniter 4",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "codeigniter",
        "dashboard",
        "codeigniter 4"
    ],
    "homepage": "https://github.com/ay4t/ci4-dashboard",
    "authors": [
        {
            "name": "Ayatulloh Ahad R",
            "email": "ayatulloh@indiega.net"
        }
    ],
    "require": {
        "lasserafn/php-initial-avatar-generator": "^4.2.1",
        "spatie/menu": "v2.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "codeigniter4/codeigniter4-standard": "^1.0",
        "squizlabs/php_codesniffer": "^3.3",
        "benedmunds/codeigniter-ion-auth": "4.x-dev",
        "ramsey/uuid": "4.2.3"
    },
    "autoload": {
        "psr-4": {
            "IDGdashboard\\": "/"
        }
    },
    "repositories": {
        "repo-name": {
            "type": "vcs",
            "url": "git@github.com:ay4t/ionauth-views.git"
        },
        "ionAuth": {
            "type": "vcs",
            "url": "git@github.com:ay4t/ionauth-views.git"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
